    "@types/node": "^7.0.5",
    "@types/react": "^15.0.12",
    "@types/react-dom": "^0.14.23",
    "ace": "^1.3.0",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "brace": "^0.10.0",
    "css-loader": "^0.28.1",
    "file-loader": "~0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "js-sha3": "^0.5.7",
    "json-loader": "^0.5.4",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.1",
    "prfun": "^2.1.4",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-ace": "^4.1.6",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "next",
    "react-tooltip": "^3.3.0",
    "redux": "^3.6.0",
