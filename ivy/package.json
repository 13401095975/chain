{
  "name": "ivy-compiler",
  "version": "0.0.1",
  "description": "Compiler for Ivy, Chain's smart contract language.",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "start": "webpack-dev-server --content-base playground/public --inline --hot",
    "test": "./node_modules/.bin/mocha-webpack --webpack-config webpack.config.js ./test/*.js",
    "bundle": "go install chain/cmd/gobundle && yarn build && gobundle -package ivy playground/public/ > ../generated/ivy/ivy.go && gofmt -w ../generated/ivy/ivy.go"
  },
  "author": "",
  "license": "",
